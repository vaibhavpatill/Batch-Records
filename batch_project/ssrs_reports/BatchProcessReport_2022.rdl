<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="BatchDataSource">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=localhost\SQLEXPRESS;Initial Catalog=BatchDB</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="BatchHeaderData">
      <Query>
        <DataSourceName>BatchDataSource</DataSourceName>
        <CommandText>
          SELECT TOP 1 
            batch_number, 
            product_name, 
            created_at,
            'System Operator' as operator_name,
            COUNT(*) OVER() as total_records
          FROM batch_data 
          WHERE batch_number = @BatchNumber
          ORDER BY created_at DESC
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@BatchNumber">
            <Value>=Parameters!BatchNumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="batch_number">
          <DataField>batch_number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="product_name">
          <DataField>product_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="created_at">
          <DataField>created_at</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="operator_name">
          <DataField>operator_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="total_records">
          <DataField>total_records</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="BatchTimeSeriesData">
      <Query>
        <DataSourceName>BatchDataSource</DataSourceName>
        <CommandText>
          SELECT 
            ROW_NUMBER() OVER (ORDER BY created_at) as second_number,
            temperature,
            pressure,
            flow_rate,
            ph_level,
            timer_value,
            state,
            FORMAT(created_at, 'HH:mm:ss') as time_stamp
          FROM batch_data 
          WHERE batch_number = @BatchNumber
          ORDER BY created_at
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@BatchNumber">
            <Value>=Parameters!BatchNumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="second_number">
          <DataField>second_number</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="temperature">
          <DataField>temperature</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="pressure">
          <DataField>pressure</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="flow_rate">
          <DataField>flow_rate</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="ph_level">
          <DataField>ph_level</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="timer_value">
          <DataField>timer_value</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="state">
          <DataField>state</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="time_stamp">
          <DataField>time_stamp</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
